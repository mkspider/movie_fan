<%= form_with model: @user, url: users_user_path, method: :patch do |f| %>

<p>プロフィール編集</p>
<table>
  <tr>
    <th>profile_image</th>
    <td><%= f.file_field :profile_image, accept: 'image/*' %></td>
  </tr>
  <tr>
    <th>profile_name</th>
    <td><%= f.text_field :name %></td>
  </tr>
  <tr>
    <th>profile_gender</th>
    <td><%= f.text_field :gender %></td>
  </tr>
  <tr>
    <th>profile_age</th>
    <td><%= f.text_field :age %></td>
  </tr>
  <tr>
    <th>email_adress</th>
    <td><%= f.text_field :email ,autofocus: true, autocomplete: "email" %></td>
  </tr>
</table>
<%= f.submit "編集内容を保存する" %>
<% end %>
